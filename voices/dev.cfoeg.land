server {

        root /var/www/dev.celestialfruit;
        index index.html index.htm index.nginx-debian.html;

        server_name land.cfoeg.land land.cfoeg.online walk.cfoeg.land walk.cfoeg.online;

        location / {
                try_files $uri $uri/ =404;
        }




    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/dev.cfoeg.land/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/dev.cfoeg.land/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = walk.cfoeg.online) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = walk.cfoeg.land) {
        return 301 https://$host$request_uri;
    } # managed by Certbot



        server_name land.cfoeg.land land.cfoeg.online walk.cfoeg.land walk.cfoeg.online;
    listen 80;
    return 404; # managed by Certbot




}